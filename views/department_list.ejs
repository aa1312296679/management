<!doctype html><html lang="en"><head>    <meta charset="UTF-8">    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">    <link rel="stylesheet" type="text/css" href="stylesheets/index.css" />    <link rel="stylesheet" type="text/css" href="stylesheets/user.css" />    <meta http-equiv="X-UA-Compatible" content="ie=edge">    <title><%=title%></title>    <script type="text/javascript" src="/javascripts/jquery-3.3.1.min.js"></script>    <script src="/javascripts/axios.min.js"></script>    <script src="/javascripts/bootstrap-paginator.js"></script>    <script>        $(function () {        /**         * 机构管理页  机构信息单删按钮点击处理         */        $(".user_del").on("click",function () {            //获取该数据行的ID            var departmentId=$(this).parents("tr").attr("title");            //获取需要删除的机构            var department=$(this).parents("tr");                        if(confirm('您确定要删除该信息吗?')){                //将该信息的信息ID提交至删除路由                $.post("/deletedepart",{departmentId:departmentId},function (data, status, xhr) {                    if(status=="error"){                        alert("删除异常");                    }else if(status=="success"){                        alert("删除成功")                        //将需要删除的机构从当前页删除                        department.remove();                    }                });            }        });        /**         * 更新按钮         */        $(".upContent").on("click",function () {            //获取该数据行的ID            var departmentId=$(this).parents("tr").attr("title");            window.location.href="/editdepart?departmentId="+departmentId;        });        /**         * 机构管理页 批量删除全选按钮         */        $("#selectall").on("click",function () {            if($(this).is(':checked')){                $("table.user").find("input.checkbox").attr("checked",true);            }else {                $("table.user").find("input.checkbox").attr("checked",false);            }        });        /**         * 机构管理页 批量删除的删除按钮点击处理         */        $("#batchdel").on("click",function () {            //获取所有被选中的机构            var inputCheckbox = $("table.user").find("input:checkbox:checked");            //所有被选中机构的机构Id            var departmentsId = new Array();            //所有被选中的机构            var departments=new Array();            //如果有机构被选中            if (inputCheckbox.length > 0) {                //遍历所有被选中的机构                inputCheckbox.each(function () {                    //将当前被选中机构的机构Id添加至机构Id数组                    departmentsId.push({departmentId: $(this).parents("tr").attr("title")});                    departments.push($(this).parents("tr"));                });            }            if(confirm("您确定要删除吗?")){                //将需要删除的信息post提交至删除路由                axios.post("/deleteAllDepartments",{departmentsId:departmentsId})                .then(res=>{                        alert(JSON.parse(res.data).result);                        //批量删除所有机构                        deleteDepartments(departments);                })                .catch(err=>{                    alert(JSON.parse(err.data).result);                    window.location.href="/departList";                })            }        });        /**         * 机构管理页 分页按钮加载         */        //获取分页容器        var element = $(".pagination");        //配置分页插件的页码参数        var options = {            //必写项：bootstrap的版本要求，即便没有引入bootstrap            bootstrapMajorVersion:3,            //设置当前页 服务器端动态获取            currentPage:$("#pageNumber").text() ,            //设置分页页码的最大显示数            numberOfPages: 5,            //设置总页数 服务器端动态获取            totalPages:$("#pageTotal").text(),            itemTexts:function (type, page, current) {                switch (type) {                    case "first":                        return "首页";                    case "prev":                        return "上一页";                    case "next":                        return "下一页";                    case "last":                        return "末页";                    case "page":                        return page;                }            },            //点击事件            onPageClicked: function (event, originalEvent, type, page) {                var curPage=Number(page);                window.location.href="/departList?page="+curPage;            }        }        //通过分页容器调用分页插件生成分页页码        element.bootstrapPaginator(options);        });        /**         * 批量删除当前页的机构信息         * @param departments 所有需要批量删除的机构         */        function deleteDepartments(departments) {            $(departments).each(function () {               $(this).remove();            });        }    </script></head><body><div class="sys_contnt">    <div class="sys_content_left">        <ul>            <li class="sys_content_left_l1"><a href="/departList" id="groupmanager">机构管理</a></li>            <li ><a id="useradd" href="/addepart">机构添加</a></li>        </ul>    </div>    <div class="sys_content_right" >        <div class="sys_user">            <h1 class="fleft">机构管理</h1>            <form action="" method="">                <input type="submit" class="s_sub" value="搜 索"/>                <input type="text" class="search" placeholder="请输入机构名"/>            </form>            <table cellpadding="0" border="0" cellspacing="0" class="user clear">                <tr class="first">                    <td width="5%"></td>                    <td width="8%">机构名称</td>                    <td width="10%">机构编码</td>                    <td width="5%">描述信息</td>                    <td width="10%">操作</td>                </tr>            <% for(var i=0;i<departmentList.length;i++){%>                <%if(i%2==0){%>                <tr title=<%=departmentList[i].departmentId%>>                    <td><input type="checkbox" class="checkbox" title="" /></td>                    <td width="5%"><%=departmentList[i].departName%></td>                    <td width="5%"><%=departmentList[i].departCode%></td>                    <td width="10%"><%=departmentList[i].departTip%></td>                    <td>                        <ul class="user_table_edit w84">                            <li><input type="button"  class="user_edit upContent"/></li>                            <li><input type="button" class="user_del"></li>                        </ul>                    </td>                </tr>                <%}else{%>                <tr class="double" title=<%=departmentList[i].departmentId%>>                    <td><input type="checkbox" class="checkbox" title=""/></td>                    <td width="5%"><%=departmentList[i].departName%></td>                    <td width="5%"><%=departmentList[i].departCode%></td>                    <td width="10%"><%=departmentList[i].departTip%></td>                    <td>                        <ul class="user_table_edit w84">                            <li><input type="button"  class="user_edit upContent"/></li>                            <li><input type="button" class="user_del" ></li>                        </ul>                    </td>                </tr>                <%}%>            <%}%>            </table>            <div class="div_page2">                <span class="w5"><input type="checkbox" class="check_all" id="selectall"/></span>                <span><a href="#" class="btn_del" id="batchdel">删 除</a></span>            </div>            <div class="pagesBtn">                <ul class="pagination">                </ul>            </div>            <!--                隐藏域                记录后台响应给客户端的当前页和总页                 以便于分页插件的调用            -->            <div id="paginationParam" style="display: none">             <span id="pageNumber"><%=pageNumber%></span>              <span id="pageTotal"><%=pageTotal%></span>            </div>            <div class="clear"></div>        </div>    </div></div></body></html>